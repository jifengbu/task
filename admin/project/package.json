    "express-graphql": "^0.4.0",
    "express-session": "^1.11.3",
    "graphql": "^0.4.7",
    "invariant": "^2.2.0",
    "jquery": "^3.1.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.2.2",
    "react-gemini-scrollbar": "^2.0.1",
    "react-google-maps": "^4.6.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.8.1",
