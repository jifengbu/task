{
  "name": "CardB_pc",
  "version": "0.0.1",
  "scripts": {
    "dist": "cross-env NODE_ENV=production webpack --config ./webpack/prod/webpack.config.js",
    "start": "cd dist && cross-env NODE_ENV=production node app",
    "startDev": "cd build && cross-env NODE_ENV=development node app",
    "dev": "parallelshell \"npm run webpackServer\" \"npm run watchServer\"",
    "watchServer": "nodemon -e js,jsx,json -w App/server -w App/shared/routers -x \"webpack --config ./webpack/dev/webpack.node.config.js && npm run startDev\"",
    "webpackServer": "webpack-dev-server --config ./webpack/dev/webpack.browser.config.js --inline --hot --quiet --display-error-details",
    "lint": "eslint --ext .jsx,.js App",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.13.3",
    "classnames": "^2.1.3",
