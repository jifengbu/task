{
    "parser": "babel-eslint",
    "extends": [
        "standard",
    ],
    "plugins": [
        "babel",
        "promise"
    ],
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "jsx-quotes": [2, "prefer-single"],
        "max-len": 0,
        "operator-linebreak": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "object-property-newline": 0,
        "one-var": 0,
        "eqeqeq": 0,
        "handle-callback-err": 0,
        "comma-dangle": [2, "always-multiline"],
        'semi': [2, 'always'],
        'object-curly-spacing': [2, 'always']
    }
}
